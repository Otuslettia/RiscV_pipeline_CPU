module M_W_Reg (
	input clk,
	input rst,
	input [31:0] dm_out,
	input [31:0] alu_out,
	input [4:0] rd_index,
	/*control signal*/
	input halt,
	input wb_sel,
	input wb_en,
	input [2:0]func3,
	output [31:0] dm_out,
	output [31:0] alu_out,
	output [4:0] rd_index,
	/*control signal*/
	output halt,
	output wb_sel,
	output wb_en,
	output [2:0]func3,
);
endmodule
